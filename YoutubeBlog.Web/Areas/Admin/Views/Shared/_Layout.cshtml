
<html lang="en" class="light-style layout-menu-fixed layout-compact " dir="ltr" data-theme="theme-default" data-template="vertical-menu-template-free">

<head>
	@Html.Partial("_Head")
</head>
<body>
	<!-- Layout wrapper -->
	<div class="layout-wrapper layout-content-navbar  ">
		<div class="layout-container">
			<!-- Menu -->
			<aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
				@Html.Partial("_Menu")
				<div class="menu-inner-shadow"></div>
				<ul class="menu-inner py-1">
					@Html.Partial("_Sidebar")
					<!-- Layout container -->
					<div class="layout-page">
						<!-- Navbar -->
						@Html.Partial("_Search")
						<ul class="navbar-nav flex-row align-items-center ms-auto">
							<!-- Place this tag where you want the button to render. -->
							<li class="nav-item lh-1 me-3">
								<a class="github-button" href="https://github.com" data-icon="octicon-star" data-size="large" data-show-count="true" aria-label="Star themeselection/sneat-html-admin-template-free on GitHub">Star</a>
							</li>
							<!-- User -->
							@Html.Partial("_User")
							<!--/ User -->
						</ul>
					</div>
					</nav>
					@RenderBody()
		</div>
	</div>
	</div>
	@Html.Partial("_Scripts")
	@await RenderSectionAsync("Scripts", false);
</body>
</html>
<!-- beautify ignore:end -->